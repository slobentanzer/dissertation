isbn = {0027-8424 (Print)$\backslash$r0027-8424 (Linking)},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
number = {15},
pages = {9005--9010},
pmid = {12855770},
title = {{The LIM-homeobox gene Lhx8 is required for the development of many cholinergic neurons in the mouse forebrain}},
