journal = {Molecular Psychiatry},
keywords = {acetylcholinesterase,acetylcholinesterase genetics,acetylcholinesterase metabolism,alternative splicing,alternative splicing physiology,animals,enzymologic,enzymologic physiolog,fear,fear physiology,gene expression regulation,hippocampus,hippocampus physiology,long term potentiation,long term potentiation genetics,memory,memory physiology,messenger,messenger genetics,mice,neuronal plasticity,neuronal plasticity genetics,physiological,physiological genetics,physiological physiopathology,protein kinase c,protein kinase c metabolism,rna,stress},
