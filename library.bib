isbn = {0091-3022},
issn = {00913022},
journal = {Frontiers in Neuroendocrinology},
keywords = {Chemokines,Cognition,Cytokines,Hippocampus,Infection,Microglia,Sensitive periods,Toll-like receptors},
number = {3},
pages = {267--286},
pmid = {22982535},
publisher = {Elsevier Inc.},
title = {{The immune system and developmental programming of brain and behavior}},
url = {http://dx.doi.org/10.1016/j.yfrne.2012.08.006},
volume = {33},
year = {2012}
}
