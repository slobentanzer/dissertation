isbn = {0022-3956},
issn = {00223956},
journal = {Journal of Psychiatric Research},
keywords = {Affective disorders,Dexamethasone/CRH test,Neuroendocrinology,Vulnerability},
month = {jan},
number = {1},
pages = {21--28},
pmid = {16018976},
title = {{The Munich vulnerability study on affective disorders: premorbid neuroendocrine profile of affected high-risk probands}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0022395604000585},
volume = {39},
year = {2005}
}
