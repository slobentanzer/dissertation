journal = {Human molecular genetics},
keywords = {Aged,Alternative Splicing,Alternative Splicing: genetics,Animals,Brain,Brain: metabolism,Cell Line, Tumor,Developmental,Exons,Female,Gene Expression Profiling,Gene Expression Regulation, Developmental,Humans,Male,Mice,MicroRNAs,MicroRNAs: genetics,MicroRNAs: metabolism,Middle Aged,Nerve Tissue Proteins,Nerve Tissue Proteins: genetics,Nerve Tissue Proteins: metabolism,Polypyrimidine Tract-Binding Protein,Polypyrimidine Tract-Binding Protein: genetics,Polypyrimidine Tract-Binding Protein: metabolism,Progressive,Progressive: genetics,Protein Isoforms,Protein Isoforms: genetics,Protein Isoforms: metabolism,Supranuclear Palsy, Progressive,Supranuclear Palsy, Progressive: genetics,Tumor,tau Proteins,tau Proteins: genetics,tau Proteins: metabolism},
