@article{Bohn2016,
abstract = {BACKGROUND Measurement of vascular density has significant value in characterizing healthy and diseased tissue, particularly in brain where vascular density varies among regions. Further, an understanding of brain vessel size helps distinguish between capillaries and larger vessels like arterioles and venules. Unfortunately, few cutting edge methodologies are available to laboratories to rapidly quantify vessel density. NEW METHOD We developed a rapid microscopic method, which quantifies the numbers and diameters of blood vessels in brain. Utilizing this method we characterized vascular density of five brain regions in both mice and rats, in two tumor models, using three tracers. RESULTS We observed the number of sections/mm(2) in various brain regions: genu of corpus callosum 161±7, hippocampus 266±18, superior colliculus 300±24, frontal cortex 391±55, and inferior colliculus 692±18 (n=5 animals). Regional brain data were not significantly different between species (p{\textgreater}0.05) or when using different tracers (70kDa and 2000kDa Texas Red; p{\textgreater}0.05). Vascular density decreased (62-79{\%}) in preclinical brain metastases but increased (62{\%}) a rat glioma model. COMPARISON WITH EXISTING METHODS Our values were similar (p{\textgreater}0.05) to published literature. We applied this method to brain-tumors and observed brain metastases of breast cancer to have a ∼2.5-fold reduction (p{\textgreater}0.05) in vessels/mm(2) compared to normal cortical regions. In contrast, vascular density in a glioma model was significantly higher (sections/mm(2) 736±84; p{\textless}0.05). CONCLUSIONS In summary, we present a vascular density counting method that is rapid, sensitive, and uses fluorescence microscopy without antibodies.},
author = {Bohn, Kaci A and Adkins, Chris E and Mittapalli, Rajendar K and Terrell-Hall, Tori B and Mohammad, Afroz S and Shah, Neal and Dolan, Emma L and Nounou, Mohamed I and Lockman, Paul R},
doi = {10.1016/j.jneumeth.2016.06.012},
file = {:Users/selo/Documents/Mendeley Desktop/Bohn et al/Journal of Neuroscience Methods/Bohn et al. - 2016 - Semi-automated rapid quantification of brain vessel density utilizing fluorescent microscopy.pdf:pdf},
issn = {01650270},
journal = {Journal of Neuroscience Methods},
keywords = {Fluorescent microscopy,Immunohistochemistry,Indocyanine green,Metastases,TX Red,Vessel density},
month = {sep},
pages = {124--131},
pmid = {27321229},
publisher = {NIH Public Access},
title = {{Semi-automated rapid quantification of brain vessel density utilizing fluorescent microscopy}},
