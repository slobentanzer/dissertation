journal = {Life sciences},
keywords = {Acetylcholine,Acetylcholine: metabolism,Cell Differentiation,Cell Differentiation: drug effects,Choline,Choline: metabolism,Cultured,Humans,Neuroblastoma,Neuroblastoma: metabolism,Neuroblastoma: pathology,Tritium,Tumor Cells, Cultured},
