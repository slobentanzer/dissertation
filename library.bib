issn = {1537-744X},
journal = {TheScientificWorldJournal},
keywords = {mirna,mirna length,northern blotting,pre mirna,single nucleotide resolution},
month = {jan},
pages = {102--117},
pmid = {21218267},
title = {{High-resolution northern blot for a reliable analysis of microRNAs and their precursors.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21218267},
volume = {11},
