isbn = {1097-4172 (Electronic)$\backslash$n0092-8674 (Linking)},
issn = {00928674},
journal = {Cell},
number = {1-2},
pages = {327--339},
pmid = {23332764},
publisher = {Elsevier Inc.},
title = {{DNA-binding specificities of human transcription factors}},
