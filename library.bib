@article{Liu2009b,
abstract = {ChAT (choline acetyltransferase) is the enzyme responsible for acetylcholine synthesis and is specifically expressed in cholinergic neurons. To further characterize the transcriptional regulation of the hCHAT (human ChAT) gene by NGF, we examined the effects upon ChAT promoter activity of a family of transcription factors which are activated by NGF and several extracellular stimuli and encoded by immediate-early genes. These include NGFI-A (Egr1, zif268), NGFI-C (Egr2), Krox-20 and NGFI-B (Nurr77). Two fragments of the hChAT gene were used for functional analysis carrying 944 bp (P1) and 4000 bp (P1 + P2) of the 5' flanking region in front of the chloramphenicol acetyltransferase (CAT) reporter gene. They were transiently co-transfected with NGFI-A, NGFI-C, Krox-20 and NGFI-B expression vectors in NG108-15, SN6 and COS-1 cells. CAT activity after transfection of the p4000 ChAT-CAT reporter into both neuronal cell lines (NG108-15 and SN6 cells) was increased up to 5-fold in the presence of co-transfected NGFI-A and up to 5- and 12-fold after co-transfection of NGFI-C expression vector in NG108-15 and SN6 cells, respectively. In NG108-15 cells, dbcAMP excerted a strong enhancing activity on the transactivation properties of NGFI-C while this was not observed when cells were transfected with NGFI-A. These trans-activation effects were specific for neuronal cells. When NG108-15 cells were treated with dbcAMP in the presence of H89, a specific PKA inhibitor, the increase of transcriptional activity of NGFI-C was abolished, indicating that a signalling transduction mechanism through PKA plays a role in NGFI-C-induced trans-activation. Electrophoretic mobility-shift assays showed that the sequence GCCCGGGGAG (NGFRE) located 1205 bp upstream of the first coding ATG (E1) can bind NGFI-A but not NGFI-C. Several possibilities explaining the observed results are discussed. Finally, transfections of ChAT-CAT reporters including the P1 + P2 region or a minimal ChAT enhancer present in the P2 region in front of a heterologous promoter indicated the presence of a regulatory element which conferred AP2-dependent trans-activation with homologous as well as with heterologous promoter constructs.},
annote = {the most devastating dementia form in the elderly, Alzheimer's disease (AD), has been shown to have the earliest and most severe pathologic changes in the entorhinal cortex and hippocampus, leading to progressive memory loss and other cognitive decline (Eustache et al., 2001). The hippocampal cholinergic hypofunction has been implicated in progressive memory deficits with both normal aging and AD (Schliebs and Arendt, 2006). However, the underlying molecular mechanisms remain largely unknown.


Our recent studies discovered that a functional deficiency of G-protein-coupled receptor kinase-5 (GRK5) occurs in early AD, which may be related to the hippocampal cholinergic dysfunctions (Suo et al., 2007, 2004). Previous studies have attributed decrements in memory associated with AD to postsynaptic M1 muscarinic acetylcholine (ACh) receptor (mAChR) and nicotinic hypofunction (Kihara and Shimohama, 2004; Ladner et al., 1995; Schliebs and Arendt, 2006).


HT4 hippocampal neurons are capable of mimicking short- and long- term potentiation (LTP) and have therefore been proposed as a good model for memory-related studies (Morimoto and Koshland, 1990).


HT22 cells, a sub-line cloned from their parent HT4 cells, have been more widely used (Aminova et al., 2005; Caldwell et al., 2007; Davis and Maher, 1994; Li et al., 1997, 1998; Lin and Takemoto, 2007; Luo and DeFranco, 2006; Panee et al., 2007; Suo et al., 2003)},
author = {Liu, Jun and Li, Longxuan and Suo, William Z},
doi = {10.1016/j.lfs.2008.12.008},
file = {:Users/selo/Documents/Mendeley Desktop/Liu, Li, Suo/Life Sciences/Liu, Li, Suo - 2009 - HT22 hippocampal neuronal cell line possesses functional cholinergic properties.pdf:pdf},
issn = {00243205},
journal = {Life Sciences},
keywords = {Acetylcholine,Acetylcholine: metabolism,Animals,Cell Differentiation,Cell Line,Cell Shape,Choline O-Acetyltransferase,Choline O-Acetyltransferase: genetics,Choline O-Acetyltransferase: metabolism,Hippocampus,Hippocampus: cytology,Inbred C57BL,Mice,Muscarinic,Muscarinic: genetics,Muscarinic: metabolism,Neurons,Neurons: cytology,Neurons: physiology,Receptors,Vesicular Acetylcholine Transport Proteins,Vesicular Acetylcholine Transport Proteins: geneti,Vesicular Acetylcholine Transport Proteins: metabo},
month = {feb},
number = {9-10},
pages = {267--271},
pmid = {19135458},
title = {{HT22 hippocampal neuronal cell line possesses functional cholinergic properties}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19135458 http://linkinghub.elsevier.com/retrieve/pii/S0024320508005006},
volume = {84},
year = {2009}
}
