pmid = {22226506},
title = {{Luteolin enhances cholinergic activities in PC12 cells through ERK1/2 and PI3K/Akt pathways.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22226506},
volume = {1437},
year = {2012}
}
