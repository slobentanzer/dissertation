keywords = {1000 genomes project,SNP,isodecoder,tRNA},
month = {dec},
number = {12},
pages = {1853--1867},
pmid = {24448271},
publisher = {Taylor {\&} Francis},
title = {{Diversity of human tRNA genes from the 1000-genomes project}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24448271},
