isbn = {0169-328X (Print)$\backslash$r0169-328X (Linking)},
issn = {0169-328X},
journal = {Brain research. Molecular brain research},
keywords = {13,14,4,acetylcholine,acutely,affect and,animals and man including,drug dependence,effects on cognition,in situ hybridisation,mood,neurotrophin,nicotine increases the release,nicotine produces numerous behavioural,of,responses in,withdrawal},
number = {1-2},
