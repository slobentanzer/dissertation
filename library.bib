issn = {1662-5099},
journal = {Frontiers in Molecular Neuroscience},
keywords = {bdnf,behaviour,cell signalling,learning and memory,synaptic plasticity,synaptogenesis},
number = {February},
pages = {1--14},
pmid = {20162032},
title = {{A simple role for BDNF in learning and memory?}},
