doi = {10.1016/j.celrep.2018.07.082},
file = {:Users/selo/Documents/Mendeley Desktop/Niwa et al/Cell Reports/Niwa et al. - 2018 - Muscarinic Acetylcholine Receptors Chrm1 and Chrm3 Are Essential for REM Sleep.pdf:pdf},
issn = {22111247},
journal = {Cell Reports},
keywords = {Chrm1,Chrm3,REM sleep,Tet system,TrkA,acetylcholine,sleep,tTR,triple-target CRISPR},
number = {9},
pages = {2231--2247.e7},
publisher = {ElsevierCompany.},
title = {{Muscarinic Acetylcholine Receptors Chrm1 and Chrm3 Are Essential for REM Sleep}},
url = {https://doi.org/10.1016/j.celrep.2018.07.082},
volume = {24},
year = {2018}
}
