issn = {00219258},
journal = {Journal of Biological Chemistry},
month = {dec},
number = {52},
pages = {33358--33365},
title = {{Cell Type-specific Regulation of Choline Acetyltransferase Gene Expression. ROLE OF THE NEURON-RESTRICTIVE SILENCER ELEMENT AND CHOLINERGIC-SPECIFIC ENHANCER SEQUENCES}},
url = {http://www.jbc.org/content/271/52/33358.full},
volume = {271},
year = {1996}
}
