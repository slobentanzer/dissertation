issn = {0021-9258},
journal = {The Journal of Biological Chemistry},
keywords = {alzheimer disease,alzheimer disease enzymology,alzheimer disease metabolism,alzheimer disease physiopathology,animals,brain,brain enzymology,electrophoresis,gel,humans,inbred c57bl,inbred dba,mass spectrometry,mice,mitochondria,mitochondria enzymology,mitochondria physiology,tau proteins,tau proteins genetics,tau proteins physiology,transgenic,two dimensional,up regulation},
