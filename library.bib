title = {{In-Silico Algorithms for the Screening of Possible microRNA Binding Sites and Their Interactions.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3637677{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {14},
