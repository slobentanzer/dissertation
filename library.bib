isbn = {1473-7175},
issn = {1744-8360},
journal = {Expert Review of Neurotherapeutics},
keywords = {acetylcholine receptor,acetyltransferase,alzheimer,anticholinesterase,basal forebrain,basalis,choline,cholinergic,galanin,gene therapy,mild cognitive impairment,ngf,nucleus,p75ntr receptor,proneurotrophin,s disease,sortilin,tau,trka receptor},
number = {11},
pages = {1703--1718},
pmid = {18986241},
title = {{Cholinergic system during the progression of Alzheimer's disease: therapeutic implications}},
