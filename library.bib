isbn = {1932-6203 (Electronic)$\backslash$r1932-6203 (Linking)},
issn = {19326203},
journal = {PLoS ONE},
number = {1},
pages = {1--6},
pmid = {20062544},
title = {{Ciliary neurotrophic factor protects striatal neurons against excitotoxicity by enhancing glial glutamate uptake}},
volume = {5},
year = {2010}
}
