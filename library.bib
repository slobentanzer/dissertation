isbn = {1522-1555 (Electronic)$\backslash$r0193-1849 (Linking)},
issn = {0193-1849},
journal = {American Journal of Physiology-Endocrinology and Metabolism},
number = {4},
pages = {E458--E467},
pmid = {22146310},
title = {{The anorexigenic cytokine ciliary neurotrophic factor stimulates POMC gene expression via receptors localized in the nucleus of arcuate neurons}},
