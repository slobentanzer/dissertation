file = {:Users/selo/Documents/Mendeley Desktop/Czubak et al/Pharmacological Reports/Czubak et al. - 2009 - Influences of chronic venlafaxine, olanzapine and nicotine on the hippocampal and cortical concentrations of brai.pdf:pdf},
isbn = {1734-1140 (Print)$\backslash$r1734-1140 (Linking)},
issn = {17341140},
journal = {Pharmacological Reports},
keywords = {Brain-derived neurotrophic factor,Nicotine,Olanzapine,Rat brain,Venlafaxine},
