doi = {10.1016/j.intimp.2015.04.058},
file = {:Users/selo/Documents/Mendeley Desktop/Nadorp, Soreq/International Immunopharmacology/Nadorp, Soreq - 2015 - Gut feeling MicroRNA discriminators of the intestinal TLR9-cholinergic links.pdf:pdf},
issn = {18781705},
journal = {International Immunopharmacology},
keywords = {Acetylcholinesterase,Butyrylcholinesterase,Intestinal inflammation,MicroRNA,Non-neuronal acetylcholine,Toll-Like Receptor 9},
number = {1},
pages = {8--14},
pmid = {26003847},
publisher = {Elsevier B.V.},
title = {{Gut feeling: MicroRNA discriminators of the intestinal TLR9-cholinergic links}},
