journal = {PLoS genetics},
keywords = {Animals,Brain,Brain: metabolism,Cell Line,Cerebellum,Cerebellum: metabolism,Cognition,Gene Expression,Gene Expression Regulation,Humans,Macaca,Macaca: genetics,Macaca: metabolism,MicroRNAs,MicroRNAs: genetics,MicroRNAs: metabolism,Microarray Analysis,Neurons,Neurons: metabolism,Pan troglodytes,Pan troglodytes: genetics,Pan troglodytes: metabolism,Phenotype,Phylogeny,Prefrontal Cortex,Prefrontal Cortex: metabolism,Selection, Genetic,Species Specificity},
month = {oct},
