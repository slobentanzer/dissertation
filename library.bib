keywords = {20th century,21st century,aging,alzheimer disease,alzheimer disease complications,atrophy,atrophy complications,atrophy pathology,brain,brain pathology,cognition disorders,cognition disorders etiology,cognition disorders history,cognition disorders psychology,dementia,dementia complications,dementia etiology,dementia history,dementia psychology,environment,history,humans,risk factors,vascular diseases,vascular diseases complications},
