isbn = {1553-7358 (Electronic)$\backslash$r1553-734X (Linking)},
issn = {15537358},
journal = {PLoS Computational Biology},
number = {3},
pmid = {24651478},
title = {{Long Non-Coding RNA and Alternative Splicing Modulations in Parkinson's Leukocytes Identified by RNA Sequencing}},
volume = {10},
year = {2014}
}
