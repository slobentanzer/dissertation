@article{Rutishauser2010,
abstract = {Learning from novel experiences is a major task of the central nervous system. In mammals, the medial temporal lobe is crucial for this rapid form of learning. The modification of synapses and neuronal circuits through plasticity is thought to underlie memory formation. The induction of synaptic plasticity is favoured by coordinated action-potential timing across populations of neurons. Such coordinated activity of neural populations can give rise to oscillations of different frequencies, recorded in local field potentials. Brain oscillations in the theta frequency range (3-8 Hz) are often associated with the favourable induction of synaptic plasticity as well as behavioural memory. Here we report the activity of single neurons recorded together with the local field potential in humans engaged in a learning task. We show that successful memory formation in humans is predicted by a tight coordination of spike timing with the local theta oscillation. More stereotyped spiking predicts better memory, as indicated by higher retrieval confidence reported by subjects. These findings provide a link between the known modulation of theta oscillations by many memory-modulating behaviours and circuit mechanisms of plasticity.},
author = {Rutishauser, Ueli and Ross, Ian B and Mamelak, Adam N and Schuman, Erin M},
doi = {10.1038/nature08860},
editor = {Array},
file = {::},
institution = {Computation and Neural Systems and Division of Biology, California Institute of Technology, Pasadena, California 91125, USA.},
issn = {1476-4687},
journal = {Nature},
keywords = {action potentials,action potentials physiology,amygdala,amygdala cytology,amygdala physiology,electrodes,epilepsy,hippocampus,hippocampus cytology,hippocampus physiology,humans,implanted,memory,memory physiology,models,neurological,neuronal plasticity,neuronal plasticity physiology,neurons,neurons physiology,theta rhythm,time factors},
month = {apr},
number = {7290},
pages = {903--907},
pmid = {20336071},
publisher = {Nature Publishing Group},
title = {{Human memory strength is predicted by theta-frequency phase-locking of single neurons.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20336071},
volume = {464},
year = {2010}
}
