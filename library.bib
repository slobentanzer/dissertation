keywords = {*alzheimer disease,*genetics,*pathology,acetylcholinesterase,amyloid beta protein precursor,animals,apoptosis,case control studies,cells,cultured,genetics,hippocampus,humans,messenger,metabolism,mice,neurons,nonparametric,pathology,peptides,rna,statistics,transgenic},
