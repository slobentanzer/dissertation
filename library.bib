isbn = {1347-8613 (Print)1347-8613 (Linking)},
journal = {Journal of pharmacological sciences},
keywords = {Acetylcholine/*immunology,Animals,Antibodies, Monoclonal/pharmacology,Antigens, Surface/immunology,Humans,Immune System Diseases/*immunology,Immunologic Factors/pharmacology,Immunotherapy,Lymphocytes/*immunology,Neurons,Simvastatin/pharmacology},
