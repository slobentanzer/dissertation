isbn = {1460-2199 (Electronic)$\backslash$r1047-3211 (Linking)},
issn = {14602199},
journal = {Cerebral Cortex},
keywords = {Acetylcholine,Alzheimer's disease,RNA metabolism,cognition,pathology},
