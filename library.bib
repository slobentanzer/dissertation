isbn = {0270-6474 (Print)$\backslash$r0270-6474 (Linking)},
issn = {0270-6474},
journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
keywords = {Animals,Cell Line,Corpus Striatum,Corpus Striatum: drug effects,Corpus Striatum: physiology,Female,Immunohistochemistry,Neurons,Neurons: physiology,Quinolinic Acid,Quinolinic Acid: pharmacology,Rats,Rats, Sprague-Dawley,Stem Cells,Stem Cells: physiology},
