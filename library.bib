title = {{MicroRNA-30c Modulates Type I IFN Responses To Facilitate Porcine Reproductive and Respiratory Syndrome Virus Infection by Targeting JAK1}},
url = {http://www.jimmunol.org/lookup/doi/10.4049/jimmunol.1502006},
volume = {196},
