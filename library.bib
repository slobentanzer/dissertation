doi = {10.1126/science.aaw7317},
file = {:Users/selo/Documents/Mendeley Desktop/Naqvi et al/Science/Naqvi et al. - 2019 - Conservation, acquisition, and functional impact of sex-biased gene expression in mammals.pdf:pdf},
issn = {0036-8075},
journal = {Science},
number = {6450},
pages = {eaaw7317},
title = {{Conservation, acquisition, and functional impact of sex-biased gene expression in mammals}},
url = {http://www.sciencemag.org/lookup/doi/10.1126/science.aaw7317},
volume = {365},
year = {2019}
}
