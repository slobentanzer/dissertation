}
@article{Mellios2011,
abstract = {This study identifies a subset of microRNAs whose expression is differentially regulated by visual experience and finds that inhibition of one of the miRNAs, miR-132, in the mouse visual cortex impairs ocular dominance plasticity. Using quantitative analyses, we identified microRNAs (miRNAs) that were abundantly expressed in visual cortex and that responded to dark rearing and/or monocular deprivation. The most substantially altered miRNA, miR-132, was rapidly upregulated after eye opening and was delayed by dark rearing. In vivo inhibition of miR-132 in mice prevented ocular dominance plasticity in identified neurons following monocular deprivation and affected the maturation of dendritic spines, demonstrating its critical role in the plasticity of visual cortex circuits.},
