doi = {10.2174/1567205015666180613111504},
file = {:Users/selo/Documents/Mendeley Desktop/Giacobini, Pepeu/Current Alzheimer Research/Giacobini, Pepeu - 2018 - Sex and Gender Differences in the Brain Cholinergic System and in the Response to Therapy of Alzheimer Disease.pdf:pdf},
issn = {15672050},
journal = {Current Alzheimer Research},
keywords = {acetylcholine,aging,cholinesterase inhibitors,estrogens,nerve growth factor,nucleus basalis magnocellularis},
number = {11},
pages = {1077--1084},
title = {{Sex and Gender Differences in the Brain Cholinergic System and in the Response to Therapy of Alzheimer Disease with Cholinesterase Inhibitors}},
volume = {15},
year = {2018}
