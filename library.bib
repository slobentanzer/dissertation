month = {may},
number = {1},
pages = {22--31},
pmid = {18442702},
title = {{Proteomic analysis of cartilage proteins.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18442702},
volume = {45},
year = {2008}
