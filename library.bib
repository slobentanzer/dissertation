month = {jan},
pages = {13},
pmid = {18946543},
publisher = {Frontiers Research Foundation},
title = {{Frequency-Dependent Gating of Synaptic Transmission and Plasticity by Dopamine}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2526279{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {1},
year = {2007}
