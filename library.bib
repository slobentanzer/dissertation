@article{Li2013b,
abstract = {A cardinal symptom of major depressive disorder (MDD) is the disruption of circadian patterns. However, to date, there is no direct evidence of circadian clock dysregulation in the brains of patients who have MDD. Circadian rhythmicity of gene expression has been observed in animals and peripheral human tissues, but its presence and variability in the human brain were difficult to characterize. Here, we applied time-of-death analysis to gene expression data from high-quality postmortem brains, examining 24-h cyclic patterns in six cortical and limbic regions of 55 subjects with no history of psychiatric or neurological illnesses ("controls") and 34 patients with MDD. Our dataset covered {\~{}}12,000 transcripts in the dorsolateral prefrontal cortex, anterior cingulate cortex, hippocampus, amygdala, nucleus accumbens, and cerebellum. Several hundred transcripts in each region showed 24-h cyclic patterns in controls, and {\textgreater}100 transcripts exhibited consistent rhythmicity and phase synchrony across regions. Among the top-ranked rhythmic genes were the canonical clock genes BMAL1(ARNTL), PER1-2-3, NR1D1(REV-ERBa), DBP, BHLHE40 (DEC1), and BHLHE41(DEC2). The phasing of known circadian genes was consistent with data derived from other diurnal mammals. Cyclic patterns were much weaker in the brains of patients with MDD due to shifted peak timing and potentially disrupted phase relationships between individual circadian genes. This transcriptome-wide analysis of the human brain demonstrates a rhythmic rise and fall of gene expression in regions outside of the suprachiasmatic nucleus in control subjects. The description of its breakdown in MDD suggests potentially important molecular targets for treatment of mood disorders.},
author = {Li, Jun Z and Bunney, Blynn G and Meng, Fan and Hagenauer, Megan H and Walsh, David M and Vawter, Marquis P and Evans, Simon J and Choudary, Prabhakara V and Cartagena, Preston and Barchas, Jack D and Schatzberg, Alan F and Jones, Edward G and Myers, Richard M and Watson, Stanley J and Akil, Huda and Bunney, William E},
doi = {10.1073/pnas.1305814110},
file = {:Users/selo/Documents/Mendeley Desktop/Li et al/Proceedings of the National Academy of Sciences of the United States of America/Li et al. - 2013 - Circadian patterns of gene expression in the human brain and disruption in major depressive disorder.pdf:pdf},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {circadian rhythms,depression,microarray},
month = {jun},
number = {24},
pages = {9950--5},
pmid = {23671070},
title = {{Circadian patterns of gene expression in the human brain and disruption in major depressive disorder.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23671070 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3683716},
volume = {110},
year = {2013}
}
