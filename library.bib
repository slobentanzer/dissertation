institution = {Karolinska Institute, Neurotec Department, Stockholm, Sweden.},
issn = {00223042},
journal = {Journal of Neurochemistry},
keywords = {1102,1113,2004,88,acetylcholinesterase,alzheimer,are currently in use,brospinal fluid,cere-,cheis,cholinesterase inhibitors,for,j,neurochem,rivastigmine,s disease,tacrine},
month = {feb},
number = {5},
pages = {1102--1113},
title = {{Long-lasting acetylcholinesterase splice variations in anticholinesterase-treated Alzheimer's disease patients.}},
