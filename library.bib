isbn = {1305901609321},
issn = {1474-760X},
journal = {Genome Biology},
keywords = {CRISPRi,Developing brain,Single-cell RNA-seq,crispri,developing brain,lncRNA,lncrna,single-cell rna-seq},
number = {1},
