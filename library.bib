issn = {10974199},
journal = {Neuron},
keywords = {abstract rules,acetylcholine,allosteric modulator,antisaccade,iontophoresis,muscarinic M1 receptor,muscarinic receptors,neurophysiology,prefrontal cortex,working memory},
pages = {1--13},
publisher = {Elsevier Inc.},
title = {{Muscarinic M1 Receptor Overstimulation Disrupts Working Memory Activity for Rules in Primate Prefrontal Cortex}},
url = {https://doi.org/10.1016/j.neuron.2018.05.027},
year = {2018}
