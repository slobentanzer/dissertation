file = {:Users/selo/Documents/Mendeley Desktop/Yuste et al/Journal of Neurochemistry/Yuste et al. - 2002 - The prevention of the staurosporine-induced apoptosis by Bcl-XL, but not by Bcl-2 or caspase inhibitors, allows th.pdf:pdf},
isbn = {0022-3042 (Print)},
issn = {00223042},
journal = {Journal of Neurochemistry},
keywords = {Bcl-2,Bcl-XL,Caspases,Neuritogenesis,SH-SY5Y cells,Staurosporine},
number = {1},
pages = {126--139},
