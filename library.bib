journal = {Molecular Neurobiology},
keywords = {15 lipoxygenase,15 lox,alzheimer,cdkn2a,cell cycle dependent kinase n2a,cfh,complement factor h,disease,interleukin 1beta associated,irak 1,irak 2,kinase 1,microrna,mirna,s},
month = {aug},
number = {1},
pages = {11--9},
pmid = {22302353},
title = {{Regulation of Complement Factor H (CFH) by Multiple miRNAs in Alzheimer's Disease (AD) Brain.}},
