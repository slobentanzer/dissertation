journal = {Nature Communications},
pages = {1145},
pmid = {23093182},
publisher = {Nature Publishing Group},
title = {{Evolution of the human-specific microRNA miR-941}},
url = {http://www.nature.com/doifinder/10.1038/ncomms2146},
volume = {3},
year = {2012}
}
