file = {:Users/selo/Documents/Mendeley Desktop/Drexhage et al/International Journal of Neuropsychopharmacology/Drexhage et al. - 2010 - Inflammatory gene expression in monocytes of patients with schizophrenia Overlap and difference with bipolar di.pdf:pdf},
isbn = {1469-5111},
issn = {14611457},
journal = {International Journal of Neuropsychopharmacology},
keywords = {Bipolar disorder,Kraepelinian dichotomy,inflammation,monocytes,schizophrenia},
number = {10},
pages = {1369--1381},
