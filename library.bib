pages = {1409--1421},
pmid = {10501184},
title = {{Extracellular-regulated kinases and phosphatidylinositol 3-kinase are involved in brain-derived neurotrophic factor-mediated survival and neuritogenesis of the neuroblastoma cell line SH-SY5Y}},
