pages = {106--127},
pmid = {23262237},
publisher = {IBRO},
title = {{Dendritic protein synthesis in the normal and diseased brain.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23262237},
volume = {232C},
year = {2012}
}
